{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "example": {
      "root": ".",
      "sourceRoot": "src/example",
      "projectType": "application",
      "prefix": "app",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/example",
            "index": "src/example/index.html",
            "main": "src/example/main.ts",
            "polyfills": "src/example/polyfills.ts",
            "tsConfig": "src/example/tsconfig.example.json",
            "assets": [
              "src/example/favicon.ico",
              "src/example/assets"
            ],
            "styles": [
              "src/example/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/example/environments/environment.ts",
                  "with": "src/example/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "material-demo:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "material-demo:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "material-demo:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/example/test.ts",
            "polyfills": "src/example/polyfills.ts",
            "tsConfig": "src/example/tsconfig.spec.json",
            "karmaConfig": "src/example/karma.conf.js",
            "styles": [
              "styles.scss"
            ],
            "scripts": [],
            "assets": [
              "src/example/favicon.ico",
              "src/example/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/example/tsconfig.app.json",
              "src/example/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "example",
  "schematics": {
    "@schematics/angular:component": {
      "prefix": "mat",
      "styleext": "scss"
    },
    "@schematics/angular:directive": {
      "prefix": "mat"
    }
  }
}
